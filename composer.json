{
    "name": "remg/generator-bundle",
    "description": "Code generation tool for Symfony3.",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.7|~3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/phpunit-bridge": "^3.2",
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
        "psr-4": { "Remg\\GeneratorBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
